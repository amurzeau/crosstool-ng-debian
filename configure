                   test -z "$ac_cv_path_BISON" && ac_cv_path_BISON=$ac_path_BISON
                   test -n "$ver" && ac_cv_path_BISON="$ac_path_BISON" ac_path_BISON_found=: ctng_version_BISON_ok=:
      $ac_path_BISON_found && break 3
    done
  done
  done
IFS=$as_save_IFS
  if test -z "$ac_cv_path_BISON"; then
    :
  fi
else
  ac_cv_path_BISON=$BISON
fi

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_path_BISON" >&5
$as_echo "$ac_cv_path_BISON" >&6; }
fi
     { $as_echo "$as_me:${as_lineno-$LINENO}: checking for bison >= 2.7" >&5
$as_echo_n "checking for bison >= 2.7... " >&6; }
     if $ctng_version_BISON_ok; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi
     BISON=$ac_cv_path_BISON

     if test -n "bison_2_7_or_newer"; then :
  if $ctng_version_BISON_ok; then :
  bison_2_7_or_newer=y
else
  bison_2_7_or_newer=
fi
          if test -n "$bison_2_7_or_newer" -o "" = "y" -o "" = "1" ; then :
  KCONFIG_bison_2_7_or_newer="def_bool y"

